{
	"name": "alariva/timegrid.io",
	"description": "Free, open-source, self-hosted online reservation platform based on the Laravel PHP Framework.",
	"keywords": ["appointments", "reservations", "booking", "agenda", "community"],
	"license": "AGPL",
	"type": "project",
	"require": {
		"alariva/tidiochat": "^1.1",
		"anhskohbo/no-captcha": "2.*",
		"barryvdh/laravel-snappy": "~0.2",
		"bassjobsen/bootstrap-3-typeahead": "^3.1",
		"caffeinated/widgets": "~2.0",
		"camroncade/timezone": "0.1",
		"creativeorange/gravatar": "~1.0",
		"fenos/notifynder": "3.*",
		"guzzlehttp/guzzle": "~5.0",
		"intervention/image": "^2.2",
		"intervention/imagecache": "^2.2",
		"ipunkt/laravel-analytics": "~1.1",
		"laracasts/flash": "~1.3",
		"laravel/framework": "5.1.*",
		"laravel/socialite": "^2.0",
		"laravelcollective/html": "~5.0",
		"mccool/laravel-auto-presenter": "^4.0",
		"patricktalmadge/bootstrapper": "~5",
		"pid/speakingurl": "^0.11.0",
		"propaganistas/laravel-phone": "~2.0",
		"sorich87/bootstrap-tour": "^0.10.2",
		"stevebauman/location": "~1.3",
		"torann/geoip": "dev-master",
		"twitter/typeahead.js": "^0.11.1",
		"webpatser/laravel-countries": "dev-master",
		"jenssegers/agent": "^2.3"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^2.0",
		"caouecs/laravel4-lang": "~2.0",
		"codeclimate/php-test-reporter": "dev-master",
		"fzaninotto/faker": "~1.0",
		"mockery/mockery": "0.9.*",
		"laracasts/generators": "^1.1",
		"laracasts/integrated": "^0.15.6",
		"laracasts/testdummy": "~2.0",
		"phpspec/phpspec": "~2.1",
		"phpunit/phpunit": "~4.0",
		"phpunit/phpunit-selenium": ">=1.2",
		"potsky/laravel-localization-helpers" : "~1.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Bootstrap\\": "bootstrap/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
