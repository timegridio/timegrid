{
    "name": "alariva/timegrid.io",
    "description": "Free, open-source, self-hosted online reservation platform based on the Laravel PHP Framework.",
    "homepage": "http://www.timegrid.io",
    "keywords": ["appointments", "reservations", "booking", "agenda", "community"],
    "license": "AGPL",
    "authors": [
        {
            "name": "Ariel Vallese",
            "email": "alariva@timegrid.io",
            "homepage": "http://alariva.com",
            "role": "Founder"
        }
    ],
    "support": {
        "issues": "https://github.com/timegridio/timegrid/issues",
        "email": "alariva@timegrid.io",
        "wiki": "https://github.com/timegridio/timegrid/wiki",
        "irc": "https://gitter.im/alariva/timegridDevelopment",
        "source": "https://github.com/timegridio/timegrid"
    },
    "type": "project",
    "require": {
        "anhskohbo/no-captcha": "2.*",
        "barryvdh/laravel-snappy": "~0.2",
        "bassjobsen/bootstrap-3-typeahead": "^3.1",
        "caffeinated/widgets": "~2.0",
        "camroncade/timezone": "0.1",
        "creativeorange/gravatar": "~1.0",
        "fenos/notifynder": "3.*",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.2",
        "intervention/imagecache": "^2.2",
        "ipunkt/laravel-analytics": "~1.1",
        "laracasts/flash": "~1.3",
        "laravel/framework": "5.2.*",
        "laravel/socialite": "^2.0",
        "laravelcollective/html": "~5.0",
        "mccool/laravel-auto-presenter": "^4.0",
        "patricktalmadge/bootstrapper": "~5",
        "pid/speakingurl": "^0.11.0",
        "propaganistas/laravel-phone": "~2.0",
        "sorich87/bootstrap-tour": "^0.10.2",
        "stevebauman/location": "~1.3",
        "torann/geoip": "dev-master",
        "twitter/typeahead.js": "^0.11.1",
        "webpatser/laravel-countries": "dev-master",
        "jenssegers/agent": "^2.3",
        "laracasts/utilities": "^2.1",
        "alariva/tidiochat": "^2.0",
        "timegridio/backend": "dev-master",
        "jeroen-g/laravel-packager": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.0",
        "caouecs/laravel4-lang": "~2.0",
        "codeclimate/php-test-reporter": "dev-master",
        "fzaninotto/faker": "~1.0",
        "laracasts/generators": "^1.1",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~4.0|~5.0",
        "phpunit/phpunit-selenium": ">=1.2",
        "potsky/laravel-localization-helpers" : "2.3.*",
        "symfony/css-selector": "~3.0",
        "symfony/dom-crawler": "~3.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Concierge\\": "app/CONCIERGE/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/helpers/"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
